# === VARIÁVEIS DE AMBIENTE PARA VERCEL ===
# Configure estas variáveis no painel do Vercel

# === APIS OBRIGATÓRIAS ===
OPENROUTER_API_KEY=your_openrouter_key_here
HUGGINGFACE_API_KEY=your_huggingface_key_here

# === ASTRA DB (OPCIONAL) ===
ASTRA_DB_TOKEN=your_astra_token_here
ASTRA_DB_ENDPOINT=your_astra_endpoint_here
ASTRA_DB_KEYSPACE=roteiro_dispensacao_bot
ASTRA_DB_COLLECTION_NAME=hanseniase_knowledge_base
ASTRA_DB_VECTOR_DIMENSION=1536
ASTRA_DB_METRIC=cosine
ASTRA_DB_HYBRID_SEARCH=true

# === CONFIGURAÇÕES DO SISTEMA ===
RATE_LIMIT_PER_MINUTE=20
RATE_LIMIT_PER_DAY=1000
CACHE_TTL_MINUTES=120
MAX_CACHE_SIZE=1000

# === CONFIGURAÇÕES DE PRODUÇÃO ===
MONITORING_ENABLED=true
CACHE_ENABLED=true
HEALTH_CHECK_ENABLED=true
METRICS_COLLECTION_ENABLED=true
ERROR_REPORTING_ENABLED=true
SECURITY_HEADERS_ENABLED=true
RATE_LIMITING_ENABLED=true

# === VERCEL ESPECÍFICO ===
VERCEL_ENV=production
VERCEL_URL=roteiro-dispensacao.vercel.app

# === AI MODELS ===
OPENROUTER_API_URL=https://openrouter.ai/api/v1
KIMIE_K2_MODEL=moonshotai/kimi-k2:free
KIMIE_K2_MODEL_FALLBACK=moonshotai/kimi-k2

# === CORS ===
CORS_ORIGINS=https://roteiro-dispensacao.vercel.app

# === LOGS ===
LOG_LEVEL=INFO