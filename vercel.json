{
  "version": 2,
  "name": "roteiro-dispensacao",
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@4.0.0"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/main.py"
    }
  ]
}