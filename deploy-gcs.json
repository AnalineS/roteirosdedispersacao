{
  "project": "roteiro-dispensacao-hanseniase",
  "bucket": "roteirosdedispensacao-com",
  "domain": "roteirosdedispensacao.com",
  "source": "./src/frontend/dist/",
  "config": {
    "defaultIndex": "index.html",
    "notFoundPage": "index.html",
    "ssl": true,
    "cdn": true,
    "gzip": true
  },
  "headers": {
    "Cache-Control": "public, max-age=31536000",
    "Content-Security-Policy": "default-src 'self' https://script.google.com https://script.googleusercontent.com 'unsafe-inline' 'unsafe-eval'",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  }
}