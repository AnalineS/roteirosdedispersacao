{
  "name": "roteiro-dispensacao-medical-platform",
  "version": "2.0.0",
  "description": "Plataforma educacional médica para hanseníase - Sistema de automação Claude",
  "main": "index.js",
  "scripts": {
    "automation:docs": "node .claude/automation/auto-documentation.js",
    "automation:lgpd": "node .claude/automation/lgpd-robust.js",
    "automation:monitor": "node .claude/automation/continuous-monitoring-system.js",
    "quality:check": "node scripts/claude-quality-check.js",
    "docs:generate": "npm run automation:docs",
    "compliance:check": "npm run automation:lgpd",
    "monitor:start": "npm run automation:monitor",
    "type-check": "cd apps/frontend-nextjs && npm run type-check",
    "lint": "cd apps/frontend-nextjs && npm run lint",
    "test:ci": "cd apps/frontend-nextjs && npm run test:ci",
    "test:accessibility": "cd apps/frontend-nextjs && npm run test:accessibility",
    "build": "cd apps/frontend-nextjs && npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "express": "^4.22.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.5.0",
    "playwright": "^1.55.1"
  },
  "devDependencies": {
    "@playwright/mcp": "^0.0.41",
    "@playwright/test": "^1.55.1",
    "@types/node": "^20.12.12",
    "@upstash/context7-mcp": "^1.0.20",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "medical",
    "hanseniase",
    "education",
    "automation",
    "claude",
    "lgpd",
    "healthcare"
  ],
  "author": "AnalineS <sousa.analine@gmail.com>",
